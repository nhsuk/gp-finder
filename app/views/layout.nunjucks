<!DOCTYPE html>
<html lang="en" class="no-js">

  <head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="DCS.dcsipa" content="1" />

    <!--[if IE]><link rel="shortcut icon" href="{{ SITE_ROOT }}/assets/images/favicon.ico"><![endif]-->
    <!-- Touch Icons - iOS and Android 2.1+ 180x180 pixels in size. -->
    <link rel="apple-touch-icon" href="{{ SITE_ROOT }}/images/apple-touch-icon.png">
    <!-- Firefox, Chrome, Safari, IE 11+ and Opera. 192x192 pixels in size. -->
    <link rel="icon" href="{{ SITE_ROOT }}/images/favicon.png">

    <title>{% if errorMessage %}Please retry - {% endif %}{% block pageTitle %}NHS.UK{% endblock %}</title>

    <meta property="og:image" content="{{ SITE_ROOT }}/images/opengraph-image.png">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="1200">
    <meta name="DCSext.ServiceName" content="GP Appointment Finder" />
    {% block meta %}{% endblock %}

    {% set qs = '?v=13' %}

    <!--[if gt IE 8]><!--><link href="{{ SITE_ROOT }}/nhsuk.css{{ qs }}" media="screen" rel="stylesheet" type="text/css"><!--<![endif]-->
    <!--[if IE 6]><link href="{{ SITE_ROOT }}/nhsuk-ie6.css{{ qs }}" media="screen" rel="stylesheet" type="text/css" /><![endif]-->
    <!--[if IE 7]><link href="{{ SITE_ROOT }}/nhsuk-ie7.css{{ qs }}" media="screen" rel="stylesheet" type="text/css" /><![endif]-->
    <!--[if IE 8]><link href="{{ SITE_ROOT }}/nhsuk-ie8.css{{ qs }}" media="screen" rel="stylesheet" type="text/css" /><![endif]-->

    <link rel="stylesheet" href="{{ SITE_ROOT }}/print.css{{ qs }}" media="print" type="text/css">

    <script type="text/javascript" src="{{ SITE_ROOT }}/js/vendor/polyfills/details.polyfill-1.0.0.min.js"></script>
    <script id="Cookiebot" src="https://consent.cookiebot.com/uc.js" data-cbid="c725d869-003e-4784-a8b8-a19cd45299ce" type="text/javascript" async></script> 

  </head>

  <body>
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
    <!-- <script>(function(d){d.className=d.className.replace(/\bno-js\b/,'js-enabled')})(document.documentElement);</script> -->
    <div class="skiplinks">
      <div class="skiplinks--inner">
        <a href="#content" class="skiplinks--link">Skip to main content</a>
      </div>
    </div>

    {% include "includes/back-to-choices.nunjucks" %}

    {% block banner %}{% endblock %}

    <header role="banner">
      <div class="global-header">
        <div class="global-header__inner">
          <a href="/" class="global-header__link" title="Go to the NHS.UK homepage">
            <img src="{{ SITE_ROOT }}/images/logotype-nhs-mono.png" alt="">
          </a>
        </div>
      </div>
    </header>

    <main id="content" role="main" class="util-nofocus">
      {% block article %}
        <div class="page-section">
          {% block header %}{% endblock %}
          {% block pageIntro %}{% endblock %}
          {% block pageSection %}
            {% block content %}{% endblock %}
          {% endblock %}
        </div>
      {% endblock %}
    </main>

    <footer role="contentinfo">
      {% include "includes/footer.nunjucks" %}
    </footer>

    {% include "includes/foot.nunjucks" %}
  </body>
</html>
